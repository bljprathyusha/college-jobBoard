<% for(var i=0;i<students.length;i++){%>
<tr>
    <td><%=students[i].rollnumber %></td>
    <td><%=students[i].name %></td>
    <td><%=students[i].stream %></td>
    <td><%=students[i].email %></td>
    <td>
        <a href="/update-user?id=<%=students[i]._id %>" class="btn border-shadow update">
            <span class="text-gradient"><i class="fa fa-pencil" aria-hidden="true"></i></span>
        </a>
        <a href="#" class="btn border-shadow delete">
            <span class="text-gradient"><i class="fa fa-trash" aria-hidden="true"></i></span>
        </a>
    </td>
</tr>
<%}%>